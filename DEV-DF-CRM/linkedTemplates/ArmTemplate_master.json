{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DEV-DF-CRM"
		},
		"Accesso_ADW_US_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Accesso_ADW_US'"
		},
		"Accesso_ADW_US_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Accesso_ADW_US'"
		},
		"DL_TSTMOBILEAPP_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DL_TSTMOBILEAPP'"
		},
		"Accesso_ADW_US_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "attractions\\KKhan_admin"
		},
		"DL_TSTMOBILEAPP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tstmobileapp.dfs.core.windows.net"
		},
		"GB01AP1063IR_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/934c0da0-cecd-4964-b5a0-ec4e9a880c9c/resourcegroups/EUW-TST-Mobile-App-Ticket-Retrieval/providers/Microsoft.DataFactory/factories/tstmobileappADF/integrationruntimes/UATEMEALONTM1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DL_TSTMOBILEAPP_accountKey": {
						"value": "[parameters('DL_TSTMOBILEAPP_accountKey')]"
					},
					"Accesso_ADW_US_connectionString": {
						"value": "[parameters('Accesso_ADW_US_connectionString')]"
					},
					"Accesso_ADW_US_password": {
						"value": "[parameters('Accesso_ADW_US_password')]"
					},
					"DL_TSTMOBILEAPP_properties_typeProperties_url": {
						"value": "[parameters('DL_TSTMOBILEAPP_properties_typeProperties_url')]"
					},
					"GB01AP1063IR_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('GB01AP1063IR_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"Accesso_ADW_US_properties_typeProperties_userName": {
						"value": "[parameters('Accesso_ADW_US_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}